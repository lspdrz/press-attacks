type User {
  id: ID! @id
  name: String!
  googleId: ID! @unique
  email: String
  avatarUrl: String
}

type Country {
  id: ID! @id
  name: String! @unique
  journalistsKilled: [Journalist!]!
}

type Journalist {
  id: ID! @id
  href: String
  cpjId: String! @unique
  fullName: String
  primaryNationality: String
  gender: String
  notes: String
  type: String
  mtpage: String
  typeOfDeath: String
  status: String
  employedAs: String
  organizations: String
  jobs: String
  coverages: String
  mediums: String
  country: Country! @relation(link: INLINE)
  location: String
  startDate: DateTime
  endDate: DateTime
  sourcesOfFire: String
  motiveConfirmed: String
  body: String  
  year: DateTime
}
